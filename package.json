{
  "name": "oauth2.0-demo",
  "version": "0.0.1",
  "description": "Repo to demonstrate understanding of OAuth2.0",
  "main": "server/index.js",
  "scripts": {
    "dev:client": "webpack --watch --config client/webpack.config.js",
    "dev:server": "nodemon --watch server",
    "dev": "run-p dev:client dev:server",
    "knex": "knex --knexfile server/db/knexfile.js",
    "webpack:server": "webpack-dev-server --hot --inline --content-base server/public --config client/webpack.config.js",
    "test": "jest --watchAll --verbose",
    "start": "node server",
    "lint": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shellymutu-grigg/oAuth2.0-demo.git"
  },
  "keywords": [
    "react",
    "OAuth"
  ],
  "author": "shellymutu-grigg",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/shellymutu-grigg/oAuth2.0-demo/issues"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-object-rest-spread"
    ]
  },
  "eslintConfig": {
    "extends": "eda"
  },
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.3",
    "@babel/preset-env": "^7.10.3",
    "@babel/preset-react": "^7.10.1",
    "@babel/register": "^7.10.3",
    "@testing-library/jest-dom": "^5.11.0",
    "@testing-library/react": "^10.4.3",
    "@testing-library/user-event": "^12.0.11",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^26.1.0",
    "babel-loader": "^8.1.0",
    "eslint": "7.13.0",
    "eslint-config-eda": "^0.1.4",
    "jest": "^26.1.0",
    "msw": "^0.19.5",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "supertest": "^4.0.2",
    "knex": "^0.21.12",
    "sqlite3": "^4.2.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "nock": "^12.0.3",
    "redux-thunk": "^2.3.0",
    "superagent": "^6.1.0"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "express": "^4.17.1",
    "material-ui-icons": "^1.0.0-beta.36"
  },
  "homepage": "https://github.com/shellymutu-grigg/oAuth2.0-demo#readme"
}
